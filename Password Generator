#include <iostream>
#include <ctime>
using namespace std;


int main()
{
	int char_random,num_random,bool_random,length = 0;
	srand(time(NULL));

	cout << " Enter Password Character Length : ";
	cin >> length;

	for (int i = 0; i < length; i++)
	{
		char_random = 33 + (rand() % 94);
		char alphabet = char_random;
		num_random = 0 + (rand() % 10);
		bool_random = 0 + (rand() % 2);
		
		if (bool_random == 1)
		{
			cout << alphabet;
		}
		else
		{
			cout << num_random;
		}
	}
}
